generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Productos {
  id                 Int      @id @default(autoincrement())
  nombre             String
  descripcion        String
  precio             Int
  fechaCreacion      DateTime @default(now())
  fechaActualizacion DateTime?
}
